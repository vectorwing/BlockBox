{
  "multipart": [
    {
      "apply": {
        "model": "blockbox:block/spiked_mangrove_palisade_post"
      }
    },
    {
      "apply": {
        "model": "blockbox:block/spiked_mangrove_palisade_front",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "blockbox:block/spiked_mangrove_palisade_front",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "blockbox:block/spiked_mangrove_palisade_back",
        "y": 270
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "blockbox:block/spiked_mangrove_palisade_back",
        "uvlock": true
      },
      "when": {
        "south": "true"
      }
    }
  ]
}