{
  "multipart": [
    {
      "apply": {
        "model": "buildingbigger:block/oak_palisade_post"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/oak_palisade_front",
        "uvlock": true
      },
      "when": {
        "north": "full"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/spiked_oak_palisade_front",
        "uvlock": true
      },
      "when": {
        "north": "spiked"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/oak_palisade_front",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "east": "full"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/spiked_oak_palisade_front",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "east": "spiked"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/oak_palisade_back",
        "uvlock": true
      },
      "when": {
        "south": "full"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/spiked_oak_palisade_back",
        "uvlock": true
      },
      "when": {
        "south": "spiked"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/oak_palisade_back",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "west": "full"
      }
    },
    {
      "apply": {
        "model": "buildingbigger:block/spiked_oak_palisade_back",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "west": "spiked"
      }
    }
  ]
}